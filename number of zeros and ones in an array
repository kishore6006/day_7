#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    int a[n], i, zc = 0, oc = 0;

    for(i = 0; i < n; i++) {
        scanf("%d", &a[i]);
        if(a[i] == 0)
            zc++;
        else if(a[i] == 1)
            oc++;
    }

    printf("zc = %d\n", zc);
    printf("oc = %d\n", oc);

    return 0;
}
